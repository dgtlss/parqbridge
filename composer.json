{
  "name": "dgtlss/parqbridge",
  "description": "Export Laravel database tables to Parquet files using Storage disks (no external deps).",
  "type": "library",
  "license": "MIT",
  "version": "1.2.0",
  "authors": [
    { "name": "Nathan Langer", "email": "nathanlanger@googlemail.com" }
  ],
  "require": {
    "php": ">=8.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "illuminate/support": "^11.0|^12.0",
    "illuminate/container": "^11.0|^12.0",
    "illuminate/config": "^11.0|^12.0",
    "illuminate/filesystem": "^11.0|^12.0",
    "illuminate/database": "^11.0|^12.0",
    "illuminate/console": "^11.0|^12.0",
    "league/flysystem": "^3.25.1",
    "phpoption/phpoption": "^1.9",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "ParqBridge\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ParqBridge\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ParqBridge\\ParqBridgeServiceProvider"
      ]
    }
  }
}
